VAR i, j, n
VAR cpt <- 0

PROGRAMME()
# Somme des n premiers entiers n(n+1)/2
# Ne pas mettre n > 10 o√π le programme tournera longtemps.
DEBUT
    n <- LIRE()
    i <- 0
    TQ i < n FAIRE
        j <- i
        TQ j < n FAIRE
            cpt <- cpt + 1
            j <- j + 1
        FTQ
        i <- i+1
        1+1
    FTQ
    ECRIRE(cpt)
FIN